<?xml version="1.0"?>
<extension key="org.civicrm.cdntaxreceipts" type="module">
  <file>cdntaxreceipts</file>
  <name>CDN Tax Receipts</name>
  <description>
      Creates Tax Receipts including single or annual/aggregate tax receipts, with distribution by email or print,
      detailed reporting, press ready PDF template possible.
  </description>
  <license>AGPL</license>
  <maintainer>
    <author>CanadaHelps</author>
  </maintainer>
  <releaseDate>2023-06-01</releaseDate>
  <version>1.5.1.2</version>
  <develStage>alpha</develStage>
  <compatibility>
    <ver>5.49</ver>
  </compatibility>
  <requires>
    <ext>com.cividesk.client.canadahelps</ext>
  </requires>
  <urls>
    <url desc="documentation">https://github.com/CanadaHelps/CDNTaxReceipts/blob/master/README.md</url>
  </urls>
  <comments>
      Customized version of CDNTaxReceipts extension (https://lab.civicrm.org/extensions/cdntaxreceipts)
  </comments>
  <civix>
    <namespace>CRM/Cdntaxreceipts</namespace>
  </civix>
</extension>
